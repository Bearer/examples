    "test": "jest --no-cache",
    "test.watch": "jest --watch --no-cache"
  },
  "dependencies": {
    "@apizi/core": "0.6.5",
    "@apizi/ui": "0.6.5"
  },
  "devDependencies": {
    "@stencil/core": "latest",
